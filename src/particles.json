{
    "fullScreen": {
      "enable": true,
      "zIndex": -999
    },
    "detectRetina": true,
    "fpsLimit": 120,
    "interactivity": {
      "events": {
        "onClick": {
          "enable": false,
          "mode": "push"
        },
        "onDiv": {
          "elementId": "repulse-div",
          "enable": true,
          "mode": "repulse"
        },
        "onHover": {
          "enable": false,
          "mode": ["grab", "bubble"],
          "parallax": {
            "enable": true,
            "force": 60,
            "smooth": 10
          }
        },
        "resize": true
      },
      "modes": {
        "bubble": {
          "distance": 400,
          "duration": 2,
          "opacity": 0.8,
          "size": 10,
          "speed": 2
        },
        "connect": {
          "distance": 80,
          "lineLinked": {
            "opacity": 0.5
          },
          "radius": 60
        },
        "grab": {
          "distance": 400,
          "lineLinked": {
            "opacity": 1
          }
        },
        "push": {
          "quantity": 4
        },
        "remove": {
          "quantity": 2
        },
        "repulse": {
          "distance": 100,
          "duration": 100
        }
      },
      "detectsOn": "window"
    },
    "particles": {
      "color": {
        "value": "#DCD7C9"
      },
      "lineLinked": {
        "blink": false,
        "color": "#DCD7C9",
        "consent": false,
        "distance": 300,
        "enable": true,
        "opacity": 0.5,
        "width": 1
      },
      "move": {
        "attract": {
          "enable": false,
          "rotate": {
            "x": 600,
            "y": 1200
          }
        },
        "bounce": true,
        "direction": "none",
        "enable": true,
        "outMode": "out",
        "random": true,
        "speed": 2,
        "straight": false
      },
      "number": {
        "density": {
          "enable": true,
          "area": 1000
        },
        "limit": 500,
        "value": 30
      },
      "opacity": {
        "animation": {
          "enable": false,
          "minimumValue": 0.1,
          "speed": 1,
          "sync": false
        },
        "random": false,
        "value": 0.5
      },
      "shape": {
        "type": "circle"
      },
      "size": {
        "animation": {
          "enable": false,
          "minimumValue": 0.1,
          "speed": 40,
          "sync": false
        },
        "random": true,
        "value": 5
      }
    },
    "polygon": {
      "draw": {
        "enable": false,
        "lineColor": "#ffffff",
        "lineWidth": 0.5
      },
      "move": {
        "radius": 100
      },
      "scale": 2,
      "type": "none",
      "url": ""
    },
    "background": {
      "color": "linear-gradient(90deg, rgba(43,51,52,1) 0%, rgba(23,63,94,1) 77%)",
      "opacity": "1",
      "image": "",
      "position": "50% 50%",
      "repeat": "no-repeat",
      "size": "cover"
    }
  }
  